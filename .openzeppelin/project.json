{
  "manifestVersion": "2.2",
  "dependencies": {},
  "name": "akropolis-pool",
  "version": "1.0.0",
  "compiler": {
    "manager": "openzeppelin",
    "compilerSettings": {
      "optimizer": {
        "enabled": true,
        "runs": "200"
      }
    },
    "artifactsDir": "build/contracts",
    "contractsDir": "contracts",
    "solcVersion": "0.5.17",
    "typechain": {
      "enabled": true,
      "outDir": "./types/truffle-contracts",
      "target": "truffle"
    }
  },
  "telemetryOptIn": true,
  "contracts": {
    "DCAModule": "DCAModule"
  }
}
